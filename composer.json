{
    "name": "presta/any-public-blend-bundle",
    "description": "A symfony 2 bundle that provides a simple way to include public 3rd-party libraries (javascripts, css, pictures ...) into your projects and keep its up-to-date.",
    "license": "MIT",
    "authors": [
        {
            "name": "David Epely",
            "email": "esion99@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/symfony": ">=2.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.22"
    },
    "autoload": {
        "psr-0": { "Presta\\AnyPublicBlendBundle": "" }
    },
    "target-dir": "Presta/AnyPublicBlendBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
